AuthType Basic
AuthName "Nouveau site JSP"
AuthUserFile /www/jsp/.htpasswd
require valid-user

Options +FollowSymlinks
RewriteEngine On
RewriteOptions MaxRedirects=10
RewriteRule (.*)shared.js $1shared.php [L]
RewriteRule ^.+/?((?:img/|fonts/|css/|js/).*)$ $1 [L]
RewriteRule ^((?!index.php|.*/?img/|.*/?fonts/|.*/?css/|.*/?js/).*)$ index.php?page=/$1 [B,L,QSA]
